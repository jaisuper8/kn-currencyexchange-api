   <xs:complexType name="UDA">
      <xs:annotation>
         <xs:documentation xml:lang="fr-FR">
                Attribut(s) défini(s) par l'utilisateur
            </xs:documentation>
         <xs:documentation xml:lang="en-US">
                User defined attribute(s)
            </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="attribute" type="xs:string">
            <xs:annotation>
               <xs:appinfo>
                        <osd:otherFacets>
                            <osd:tableRef>
                                <tablePath>//udaCatalog</tablePath>
                            </osd:tableRef>
                        </osd:otherFacets>
                    </xs:appinfo>
            </xs:annotation>
         </xs:element>
         <xs:element name="value">
            <xs:complexType>
               <xs:sequence>
                  <xs:element minOccurs="0" name="xsString" type="xs:string"/>
                  <xs:element minOccurs="0" name="xsBoolean" type="xs:boolean"/>
                  <xs:element minOccurs="0" name="xsInt" type="xs:int"/>
                  <xs:element minOccurs="0" name="xsDecimal" type="xs:decimal"/>
                  <xs:element minOccurs="0" name="xsDateTime" type="xs:dateTime"/>
                  <xs:element minOccurs="0" name="xsDate" type="xs:date"/>
                  <xs:element minOccurs="0" name="xsTime" type="xs:time"/>
                  <xs:element minOccurs="0" name="xsName" type="xs:Name"/>
                  <xs:element minOccurs="0" name="xsAnyURI" type="xs:anyURI"/>
                  <xs:element minOccurs="0" name="osdText" type="osd:text"/>
                  <xs:element minOccurs="0" name="osdHtml" type="osd:html"/>
                  <xs:element minOccurs="0" name="osdEmail" type="osd:email"/>
                  <xs:element minOccurs="0" name="osdPassword" type="osd:password"/>
                  <xs:element minOccurs="0" name="osdLocale" type="osd:locale"/>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="UDACatalog">
      <xs:annotation>
         <xs:appinfo>
                <osd:table>
                    <primaryKeys>/name</primaryKeys>
                </osd:table>
            </xs:appinfo>
         <xs:documentation xml:lang="fr-FR">
                Catalogue des attributs. Catalogue des attributs définis
                par l'utilisateur.
            </xs:documentation>
         <xs:documentation xml:lang="en-US">
                Attributes catalog. Catalog for user defined attributes.
            </xs:documentation>
      </xs:annotation>
      <xs:sequence>
         <xs:element name="name" type="xs:Name"/>
         <xs:element name="type">
            <xs:simpleType>
               <xs:restriction base="xs:string">
                  <xs:enumeration value="xs:string"/>
                  <xs:enumeration value="xs:boolean"/>
                  <xs:enumeration value="xs:int"/>
                  <xs:enumeration value="xs:decimal"/>
                  <xs:enumeration value="xs:dateTime"/>
                  <xs:enumeration value="xs:date"/>
                  <xs:enumeration value="xs:time"/>
                  <xs:enumeration value="xs:Name"/>
                  <xs:enumeration value="xs:anyURI"/>
                  <xs:enumeration value="osd:text"/>
                  <xs:enumeration value="osd:html"/>
                  <xs:enumeration value="osd:email"/>
                  <xs:enumeration value="osd:password"/>
                  <xs:enumeration value="osd:locale"/>
               </xs:restriction>
            </xs:simpleType>
         </xs:element>
         <xs:element name="documentation" osd:access="RW">
            <xs:annotation>
               <xs:documentation xml:lang="fr-FR">
                        Libellé et description. Libellés et descriptions
                        internationalisés, ils sont affichés à
                        l'utilisateur de EBX.Manager.
                    </xs:documentation>
               <xs:documentation xml:lang="en-US">
                        Label and description. Internationalized labels
                        and descriptions, they are displayed to end user
                        in EBX.Manager.
                    </xs:documentation>
            </xs:annotation>
            <xs:complexType>
               <xs:annotation>
                  <xs:documentation xml:lang="fr-FR">
                            Libellé et description
                        </xs:documentation>
                  <xs:documentation xml:lang="en-US">
                            Label and description
                        </xs:documentation>
               </xs:annotation>
               <xs:sequence>
                  <xs:element maxOccurs="unbounded" minOccurs="0" name="localizedDocumentations">
                     <xs:annotation>
                        <xs:documentation xml:lang="fr-FR">
                                    [locale]
                                </xs:documentation>
                        <xs:documentation xml:lang="en-US">
                                    [locale]
                                </xs:documentation>
                     </xs:annotation>
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="locale" type="osd:locale">
                              <xs:annotation>
                                 <xs:documentation xml:lang="fr-FR">
                                                Région
                                            </xs:documentation>
                                 <xs:documentation xml:lang="en-US">
                                                Locale
                                            </xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element minOccurs="0" name="label" type="xs:string">
                              <xs:annotation>
                                 <xs:documentation xml:lang="fr-FR">
                                                Libellé
                                            </xs:documentation>
                                 <xs:documentation xml:lang="en-US">
                                                Label
                                            </xs:documentation>
                              </xs:annotation>
                           </xs:element>
                           <xs:element minOccurs="0" name="description">
                              <xs:annotation>
                                 <xs:documentation xml:lang="fr-FR">
                                                Description
                                            </xs:documentation>
                                 <xs:documentation xml:lang="en-US">
                                                Description
                                            </xs:documentation>
                              </xs:annotation>
                              <xs:simpleType>
                                 <xs:restriction base="xs:string">
                                    <xs:maxLength value="99999"/>
                                 </xs:restriction>
                              </xs:simpleType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
</xs:schema>
